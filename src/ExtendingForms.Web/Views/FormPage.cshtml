@using Umbraco.Cms.Web.Common.PublishedModels;
@using Umbraco.Forms.Web
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.FormPage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "master.cshtml";

	var formGuid = Model.Form;
}

@Html.Partial("~/Views/Partials/SectionHeader.cshtml")

<section class="section">

	<div class="container">

		<div class="col-md-3">
			<nav class="nav-bar nav-bar--list">
				@Html.Partial("~/Views/Partials/Navigation/SubNavigation.cshtml")
			</nav>
		</div>

		<div class="col-md-9">
			<article>
				@if (formGuid is not null)
				{
					@await Component.InvokeAsync("RenderForm", new { formId = formGuid, theme = "default", includeScripts = false })
				}
			</article>
		</div>
	</div>

</section>

@Html.RenderUmbracoFormDependencies(Url)

<umb-forms-render-scripts theme="default" />